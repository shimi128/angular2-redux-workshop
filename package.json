    "node-sass": "^3.8.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "to-string-loader": "1.1.4",
    "typescript": "1.8.9",
    "typings": "1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-merge": "0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
